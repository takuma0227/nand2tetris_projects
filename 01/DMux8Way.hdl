/**
 * 8-way demultiplexor:
 * {a, b, c, d, e, f, g, h} = {in, 0, 0, 0, 0, 0, 0, 0} if sel == 000
 *                            {0, in, 0, 0, 0, 0, 0, 0} if sel == 001
 *                            etc.
 *                            {0, 0, 0, 0, 0, 0, 0, in} if sel == 111
 */

CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux4Way(in=in, sel=sel[0..1], a=w1, b=w2, c=w3, d=w4);
    DMux(in=in, sel=sel[2], a=w5, b=w6);
    And(a=w1, b=w5, out=a);
    And(a=w2, b=w5, out=b);
    And(a=w3, b=w5, out=c);
    And(a=w4, b=w5, out=d);
    And(a=w1, b=w6, out=e);
    And(a=w2, b=w6, out=f);
    And(a=w3, b=w6, out=g);
    And(a=w4, b=w6, out=h);
}